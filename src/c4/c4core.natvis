  <Type Name="c4::stg::raw_paged&lt;*,*,*,*,*&gt;">
    <DisplayString>{{pg_sz={$T3} #pgs={m_numpages_n_alloc.m_value}}}</DisplayString>
    <Expand>
      <Item Name="[capacity]">$T3 * m_numpages_n_alloc.m_value</Item>
      <Item Name="[page size]">$T3</Item>
      <Item Name="[num pages]">m_numpages_n_alloc.m_value</Item>
